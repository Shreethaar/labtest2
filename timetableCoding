package quiz;
import java.util.Scanner;

public class TimeTable {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num;
		System.out.println("Enter a size");	
		num=sc.nextInt();
		for (int i=1;i<=num;i++){
			  for (int j=1;j<=num;j++)
			     System.out.print("\t"+i*j);
			  System.out.println(); 
			}
		

		
		
	}

}
